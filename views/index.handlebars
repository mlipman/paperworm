		<div class="row" style="padding-top: 10px; padding-bottom: 10px">
			<!--Paper Cover-->
			<div class="col-sm-3">
				<div class="paper-container">
					<a href="read" height="600" width="400" ><img src="paperImg.png" class="img-responsive paper-image" height="600" width="400"/></a>
				</div>
			</div>
			<!--Paper Details-->
			<div class="col-sm-9">
				<div class="paper-details">
					<h2>SCADS: A Model of Childrenâ€™s Strategy Choices and Strategy Discoveries</h2>
					<h4 id="authors">Jeff Shrager and Robert S. Siegler</h4>
					<!--Paper information-->
					<div class="table-responsive">
						<table class="table-striped table-bordered">
							<tr>
								<td><b>Journal</b></td>
								<td>{{ details.journal }}</td>
							</tr>
							<tr>
								<td><b>Publication Date</b></td>
								<td>{{ details.pubdate }}</td>
							</tr>
							<tr>
								<td><b>Pages</b></td>
								<td>{{ details.pages }}</td>
							</tr>
							<tr>
								<td><b>Abstract</b></td>
								<td><p>{{ details.abstract }}</p></td>
							</tr>
						</table>
					</div>
					<div class="citation-modal">
						<!-- Button trigger Citation modal -->
						<button class="btn btn-primary btn-sm" data-toggle="modal" data-target="#citationModal">
							APA Citation
						</button>

						<!-- Citation Modal -->
						<div class="modal fade" id="citationModal" tabindex="-1" role="dialog" aria-labelledby="myModalLabel" aria-hidden="true">
							<div class="modal-dialog">
								<div class="modal-content">
									<div class="modal-header">
										<button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
										<h4 class="modal-title" id="myModalLabel">APA</h4>
									</div>
									<div class="modal-body">
										<p>{{ details.citation }}</p>
									</div>
									<div class="modal-footer">
										<button type="button" class="btn btn-primary">Copy</button>
										<button type="button" class="btn btn-default" data-dismiss="modal">Close</button>
									</div>
								</div>
							</div>
						</div>
					</div>
				</div> <!--End of paperdetails-->
			</div>
		</div>

		<div>

			<ul class="nav nav-tabs nav-justified">
				<li class="active"><a href="#notes" data-toggle="tab">Notes</a></li>
				<li><a href="#glossary" data-toggle="tab">Glossary</a></li>
				<li><a href="#highlights" data-toggle="tab">Highlights</a></li>
			</ul>

		</div>

		<div class="tab-content">
			<div class="tab-pane fade in active" id="notes">

				{{! a space before #each will cause errors }}
				{{#each notes}}
					<!-- Note from student -->
					<div class="panel panel-default">
						<!-- Note header -->
						<div class="panel-heading">
							<div class="row">
								<!-- Title -->
								<div class="col-sm-10">
									<h4 class="panel-title">Note from {{ author }}</h4>
								</div>
								<!-- Buttons -->
								<div class="col-sm-2">
									<div class="btn-group" style="float: right">
										<button type="button" class="btn btn-default btn-xs" id="edit-obj"><span class="glyphicon glyphicon-pencil"></span></button>
										<button type="button" class="btn btn-default btn-xs" id="remove-obj"><span class="glyphicon glyphicon-remove"></span></button>
									</div>
								</div>
							</div>
						</div>
						<!-- Note content -->
						<div class="panel-body">
							<p>{{ body }}</p>
						</div>
					</div>
				{{/each}}



